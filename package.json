{
  "name": "m2",
  "version": "0.0.1",
  "description": "TypegraphQL boilerplate Mark 2",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "transpile": "tsc",
    "optimize": "webpack",
    "build": "npm run transpile && npm run optimize",
    "start": "pm2-runtime start dist/server.bundle.js -i 4 --name m2",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "test": "jest --passWithNoTests",
    "test-lint": "ts-standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorstein/M2.git"
  },
  "keywords": [
    "Alfonso",
    "Gomez",
    "Nicaragua",
    "TypeScript",
    "GraphQL",
    "TypeGraphQL",
    "TypeORM",
    "BoilerPlate"
  ],
  "prisma": {
    "schema": "src/db/schema.prisma"
  },
  "author": "Alfonso Gomez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victorstein/M2/issues"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "jest.config.ts",
      "webpack.config.ts"
    ]
  },
  "homepage": "https://github.com/victorstein/M2#readme",
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "@sentry/node": "^6.3.6",
    "apollo-server-express": "^2.24.0",
    "argon2": "^0.27.2",
    "class-validator": "^0.13.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.3",
    "helmet": "^4.6.0",
    "pm2": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-sslify": "^1.2.1",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/tapable": "^2.2.2",
    "@types/webpack-node-externals": "^2.5.1",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.16",
    "prisma": "^2.22.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "ts-standard": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typegraphql-prisma": "^0.14.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
}
